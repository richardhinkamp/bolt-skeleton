{
    "name": "richardhinkamp/bolt-standard",
    "description": "Standard edition of Bolt install with Bolt as dependency",
    "license": "MIT",
    "autoload": {
        "psr-0": { "": "src/" }
    },
    "require": {
        "htmlawed/htmlawed": "dev-master",
        "hautelook/phpass": "dev-master",
        "garetjax/phpbrowscap": "*@dev",
        "doctrine/dbal": "2.4.*@dev",
        "doctrine/common": "2.4.*@dev",
        "bolt/bolt": "dev-master"
    },
    "require-dev": {
    },
    "minimum-stability": "stable",
    "scripts": {
        "post-install-cmd": [
            "Bolt\\Composer\\ScriptHandler::installAssets"
        ],
        "post-update-cmd": [
            "Bolt\\Composer\\ScriptHandler::installAssets"
        ]
    },
    "extra": {
        "bolt-web-dir": "web",
        "bolt-dir-mode": "0755"
    }
}
